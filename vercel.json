{
    "functions": {
      "api/**/*": {
        "memory": 3008,
        "maxDuration": 60
      }
    },
    "regions": ["cdg1"],
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, max-age=0"
          }
        ]
      }
    ]
  }